<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="MBG" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressAllComments" value="true"/><!-- 是否取消注释 -->  
			<property name="suppressDate" value="true" /> <!-- 是否生成注释代时间戳-->  
		</commentGenerator>
		
       <jdbcConnection driverClass="${driverClass}" connectionURL="${connectionURL}"
            userId="${userId}" password="${password}">
        </jdbcConnection>
         <!-- 类型转换 -->  
        <javaTypeResolver>
             <!-- 是否使用bigDecimal， false可自动转化以下类型（Long, Integer, Short, etc.） -->  
            <property name="forceBigDecimals" value="false"/>  
        </javaTypeResolver>
        
        <javaModelGenerator targetPackage="com.inkyi.iblog.entity" targetProject="${targetProject}">
            <!-- 是否在当前路径下新加一层schema,eg：fase路径com.oop.eksp.user.model， true:com.oop.eksp.user.model.[schemaName] -->  
            <property name="enableSubPackages" value="false" />
            <!-- 是否针对string类型的字段在set的时候进行trim调用 -->  
            <property name="trimStrings" value="false" />
        </javaModelGenerator>
        
        <sqlMapGenerator targetPackage="com.inkyi.iblog.mapper" targetProject="${targetProject}">
            <!-- 是否在当前路径下新加一层schema,eg：fase路径com.oop.eksp.user.model， true:com.oop.eksp.user.model.[schemaName] --> 
            <property name="enableSubPackages" value="false" />
        </sqlMapGenerator>
        
         <!-- 生成mapxml对应client，也就是接口dao -->      
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.inkyi.iblog.dao" targetProject="${targetProject}">
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>
        
         <table schema="" tableName="ib_album" domainObjectName="IbAlbum">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_article" domainObjectName="IbArticle">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_article_group" domainObjectName="IbArticleGroup">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_article_reply" domainObjectName="IbArticleReply">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_auth" domainObjectName="IbAuth">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_dictionary" domainObjectName="IbDictionary">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_menu" domainObjectName="IbMenu">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_msg" domainObjectName="IbMsg">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_photo" domainObjectName="IbPhoto">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_photo_reply" domainObjectName="IbPhotoReply">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_role" domainObjectName="IbRole">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_role_auth" domainObjectName="IbRoleAuth">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_statistics" domainObjectName="IbStatistics">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_user" domainObjectName="IbUser">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_user_info" domainObjectName="IbUserInfo">
             <property name="useActualColumnNames" value="false" />
         </table>
         
         <table schema="" tableName="ib_user_role" domainObjectName="IbUserRole">
             <property name="useActualColumnNames" value="false" />
         </table>
         
    </context>
</generatorConfiguration>
